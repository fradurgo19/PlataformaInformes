{
  "version": 2,
  "routes": [
    { "src": "/api/(.*)", "dest": "backend/src/index.ts" },
    { "src": "/assets/(.*)", "dest": "dist/assets/$1" },
    { "src": "/(.*\\.js)", "dest": "dist/$1" },
    { "src": "/(.*\\.css)", "dest": "dist/$1" },
    { "src": "/(.*\\.png)", "dest": "dist/$1" },
    { "src": "/(.*\\.svg)", "dest": "dist/$1" },
    { "src": "/(.*\\.ico)", "dest": "dist/$1" },
    { "src": "/(.*)", "dest": "dist/index.html" }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 