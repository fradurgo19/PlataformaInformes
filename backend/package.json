{
  "name": "machinery-reports-backend",
  "version": "1.0.0",
  "description": "Backend API for Machinery Reports Platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "start": "npm run build && node dist/index.js",
    "build": "tsc",
    "postinstall": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:init": "node init-db.js",
    "db:reset": "node reset-db.js",
    "db:seed": "node seed-data.js",
    "db:clean": "node clean-test-data.js",
    "db:clean:prod": "node clean-production-data.js",
    "db:stats": "node clean-test-data.js --stats"
  },
  "keywords": [
    "machinery",
    "reports",
    "api",
    "postgresql"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sparticuz/chromium": "^138.0.1",
    "@supabase/supabase-js": "^2.39.0",
    "@types/nodemailer": "^6.4.17",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.3",
    "pg": "^8.11.3",
    "puppeteer": "^24.10.1",
    "puppeteer-core": "^24.14.0",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.3",
    "jest": "^30.0.4",
    "nodemon": "^3.0.2",
    "supertest": "^7.1.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2"
  }
